{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": [
    "company_id",
    "created_at",
    "updated_at",
    "organization_id"
  ],
  "properties": {
    "company_id": {
      "type": "string",
      "format": "hostname"
    },
    "organization_id": {
      "type": "string",
      "pattern": "^[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{12}$"
    },
    "created_at": {
      "type": "integer",
      "minimum": 0
    },
    "updated_at": {
      "type": "integer",
      "minimum": 0
    }
  }
}
