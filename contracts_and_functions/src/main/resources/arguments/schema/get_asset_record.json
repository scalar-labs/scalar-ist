{
  "$schema": "https://json-schema.org/draft-07/schema#",
  "description": "Get Asset Record contract arguments schema",
  "type": "object",
  "required": [
    "asset_id",
    "is_hashed"
  ],
  "properties": {
    "asset_id": {
      "type": "string",
      "pattern": "^[a-zA-Z0-9-/_]+$"
    },
    "is_hashed": {
      "type": "boolean"
    }
  }
}
