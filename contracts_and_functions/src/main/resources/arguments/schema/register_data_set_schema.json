{
  "$schema": "https://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": [
    "company_id",
    "organization_id",
    "holder_id",
    "data_set_name",
    "description",
    "data_location",
    "category_of_data",
    "data_type",
    "classification",
    "data_set_schema",
    "changes",
    "created_at"
  ],
  "properties": {
    "company_id": {
      "type": "string",
      "format": "hostname"
    },
    "organization_id": {
      "type": "string",
      "pattern": "^[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{12}$"
    },
    "data_set_name": {
      "type": "string",
      "minLength": 1
    },
    "description": {
      "type": "string",
      "minLength": 1
    },
    "data_location": {
      "type": "string",
      "minLength": 1
    },
    "category_of_data": {
      "type": "array",
      "items": {
        "type": "string",
        "minLength": 1
      }
    },
    "data_type": {
      "type": "array",
      "items": {
        "type": "string",
        "minLength": 1
      }
    },
    "classification": {
      "type": "array",
      "items": {
        "type": "string",
        "minLength": 1
      }
    },
    "data_set_schema": {
      "type": "string",
      "minLength": 1
    },
    "changes": {
      "type": "string",
      "minLength": 1
    },
    "created_at": {
      "type": "integer",
      "minimum": 0
    }
  }
}
