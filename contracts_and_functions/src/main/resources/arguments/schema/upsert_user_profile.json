{
  "$schema": "https://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": [
    "executor_company_id",
    "company_id",
    "organization_ids",
    "roles",
    "holder_id",
    "created_at",
    "updated_at",
    "mode"
  ],
  "properties": {
    "executor_company_id": {
      "type": "string",
      "format": "hostname"
    },
    "company_id": {
      "type": "string",
      "format": "hostname"
    },
    "organization_ids": {
      "type": "array",
      "items": {
        "type": "string",
        "pattern": "^[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{12}$"
      }
    },
    "roles": {
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "string",
        "enum": [
          "Administrator",
          "Processor",
          "Controller"
        ]
      }
    },
    "holder_id": {
      "type": "string",
      "pattern": "^[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{12}$"
    },
    "created_at": {
      "type": "integer",
      "minimum": 0
    },
    "updated_at": {
      "type": "integer",
      "minimum": 0
    },
    "mode": {
      "type": "string",
      "enum": [
        "insert",
        "update"
      ]
    }
  }
}
