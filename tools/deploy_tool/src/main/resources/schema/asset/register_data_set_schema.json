{
  "contract_argument_schema":{
    "$schema":"http://json-schema.org/draft-07/schema#",
    "type": "object",
    "required": [
      "action",
      "company_id",
      "organization_id",
      "data_set_name",
      "description",
      "category_of_data",
      "data_type",
      "classification",
      "data_set_schema",
      "changes",
      "created_at"
    ],
    "properties": {
      "action":{
        "type":"string",
        "enum":[
          "insert"
        ]
      },
      "company_id": {
        "type": "string",
        "format": "hostname"
      },
      "organization_id": {
        "type": "string",
        "pattern": "^[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{12}$"
      },
      "data_set_name": {
        "type": "string",
        "minLength": 1
      },
      "description": {
        "type": "string",
        "minLength": 1
      },
      "category_of_data": {
        "type": "array",
        "minLength": 1
      },
      "data_type": {
        "type": "array",
        "minLength": 1
      },
      "classification": {
        "type": "array",
        "minLength": 1
      },
      "data_set_schema": {
        "type": "object",
        "minLength": 1
      },
      "changes": {
        "type": "string",
        "minLength": 1
      },
      "created_at": {
        "type": "integer",
        "minimum": 0
      }
    }
  },
  "asset_name": "ds",
  "asset_version": "01",
  "asset_schema": {
    "$schema":"http://json-schema.org/draft-07/schema#",
    "type":"object",
    "properties":{
      "data_set_schema_id":{
        "type":"string",
        "pattern": "asset_id"
      },
      "company_id":{
        "type":"string"
      },
      "organization_id":{
        "type":"string"
      },
      "data_set_name":{
        "type":"string"
      },
      "description":{
        "type":"string"
      },
      "category_of_data":{
        "type":"array"
      },
      "data_type":{
        "type":"array"
      },
      "classification":{
        "type":"array"
      },
      "data_set_schema":{
        "type":"object"
      },
      "changes":{
        "type":"string"
      }
    }
  },
  "table_schema": {
    "$schema": "scalar_db_table_schema.json",
    "table_name": "data_set_schema",
    "partition_keys": [
      {
        "name": "company_id",
        "type": "text"
      }
    ],
    "clustering_keys": [
      {
        "name": "created_at",
        "type": "bigint"
      },
      {
        "name": "organization_id",
        "type": "text"
      }
    ],
    "columns": [
      {
        "name": "data_set_name",
        "type": "text"
      },
      {
        "name": "description",
        "type": "text"
      },
      {
        "name": "data_location",
        "type": "text"
      },
      {
        "name": "category_of_data",
        "type": "text"
      },
      {
        "name": "classification",
        "type": "text"
      },
      {
        "name": "data_set_schema",
        "type": "text"
      },
      {
        "name": "is_active",
        "type": "boolean"
      },
      {
        "name": "updated_at",
        "type": "bigint"
      }
    ]
  }
}