{
  "contract_argument_schema":{
    "$schema":"http://json-schema.org/draft-07/schema#",
    "type":"object",
    "required":[
      "executor_company_id",
      "company_id",
      "company_name",
      "company_metadata",
      "organization_id",
      "created_at"
    ],
    "properties":{
      "executor_company_id":{
        "type":"string",
        "format":"hostname"
      },
      "company_id":{
        "type":"string",
        "format":"hostname"
      },
      "company_name":{
        "type":"string",
        "pattern":"^.+$"
      },
      "corporate_number":{
        "type":"string",
        "pattern":"^[0-9]{0,13}$"
      },
      "company_metadata":{
        "type":"object"
      },
      "organization_id":{
        "type":"string",
        "pattern":"^[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{12}$"
      },
      "created_at":{
        "type":"integer",
        "minimum":0
      }
    }
  },
  "asset_name": "cp",
  "asset_version": "01"
}